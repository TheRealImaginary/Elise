{
  "name": "elise",
  "version": "1.0.0",
  "description": "A bot for discord.",
  "main": "elise.js",
  "scripts": {
    "start": "node --harmony elise.js"
  },
  "keywords": [
    "bot",
    "discord.js",
    "nodejs",
    "discord"
  ],
  "author": "Mazen Magdy",
  "license": "",
  "dependencies": {
    "axios": "^0.16.1",
    "discord.js": "^11.1.0",
    "discord.js-commando": "^0.9.0",
    "html-to-text": "^3.2.0"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "rules": {
      "no-unused-vars": [
        "warn"
      ],
      "object-curly-spacing": [
        "warn"
      ],
      "indent": [
        "error",
        2
      ]
    }
  }
}
