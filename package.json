{
  "name": "elise",
  "version": "1.0.0",
  "description": "A bot for discord.",
  "main": "elise.js",
  "scripts": {
    "start": "node --harmony elise.js"
  },
  "keywords": [
    "bot",
    "discord.js",
    "nodejs",
    "discord"
  ],
  "author": "Mazen Magdy",
  "license": "",
  "dependencies": {
    "Sherlock": "github:neilgupta/Sherlock",
    "axios": "^0.16.1",
    "discord.js": "github:hydrabolt/discord.js",
    "discord.js-commando": "^0.9.0",
    "html-to-text": "^3.2.0",
    "moment": "^2.18.1",
    "mongoose": "^4.9.9",
    "node-opus": "^0.2.6",
    "simple-youtube-api": "^2.1.0",
    "ytdl-core": "^0.14.0"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "rules": {
      "no-unused-vars": [
        "warn"
      ],
      "object-curly-spacing": [
        "warn"
      ],
      "indent": [
        "error",
        2
      ]
    }
  }
}
